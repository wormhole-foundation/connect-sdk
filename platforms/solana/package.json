{
  "name": "@wormhole-foundation/connect-sdk-solana",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wormhole-foundation/connect-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/wormhole-foundation/connect-sdk/issues"
  },
  "homepage": "https://github.com/wormhole-foundation/connect-sdk#readme",
  "directories": {
    "test": "__tests__"
  },
  "license": "Apache-2.0",
  "version": "0.1.0-beta.1",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "author": "",
  "description": "SDK for Solana, used in conjunction with @wormhole-foundation/connect-sdk",
  "files": [
    "dist/**/*",
    "src/**/*"
  ],
  "keywords": [
    "wormhole",
    "sdk",
    "typescript",
    "connect",
    "solana"
  ],
  "engines": {
    "node": ">=16"
  },
  "sideEffects": false,
  "scripts": {
    "build:cjs": "tsc -p ./tsconfig.cjs.json",
    "build:esm": "tsc -p ./tsconfig.esm.json",
    "build": "npm run build:cjs && npm run build:esm",
    "rebuild": "npm run clean && npm run build:cjs && npm run build:esm",
    "test": "jest --config ./jest.config.ts",
    "clean": "rm -rf ./dist && rm -f ./*.tsbuildinfo",
    "lint": "npm run prettier && eslint --fix",
    "prettier": "prettier --write ./src"
  },
  "devDependencies": {
    "@types/bs58": "^4.0.1",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.7.0",
    "lint-staged": "^13.1.2",
    "prettier": "2.8.4",
    "typescript": "5.1.6"
  },
  "dependencies": {
    "@coral-xyz/borsh": "0.2.6",
    "@project-serum/anchor": "0.25.0",
    "@project-serum/borsh": "0.2.5",
    "@solana/spl-token": "0.3.5",
    "@solana/web3.js": "1.66.2",
    "@wormhole-foundation/connect-sdk": "*",
    "bs58": "^4.0.1",
    "nock": "^13.3.3"
  }
}
