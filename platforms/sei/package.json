{
  "name": "@wormhole-foundation/connect-sdk-sei",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wormhole-foundation/connect-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/wormhole-foundation/connect-sdk/issues"
  },
  "homepage": "https://github.com/wormhole-foundation/connect-sdk#readme",
  "directories": {
    "test": "tests"
  },
  "license": "Apache-2.0",
  "version": "0.1.0-beta.1",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "author": "",
  "description": "SDK for SEI, used in conjunction with @wormhole-foundation/connect-sdk",
  "files": [
    "dist/**/*",
    "src/**/*"
  ],
  "keywords": [
    "wormhole",
    "sdk",
    "typescript",
    "connect",
    "sei"
  ],
  "engines": {
    "node": ">=16",
    "pnpm": ">=7"
  },
  "sideEffects": false,
  "scripts": {
    "build:cjs": "tsc -p ./tsconfig.cjs.json",
    "build:esm": "tsc -p ./tsconfig.esm.json",
    "build": "npm run clean && npm run build:cjs && npm run build:esm",
    "clean": "rm -rf ./dist && rm -f ./*.tsbuildinfo",
    "lint": "npm run prettier && eslint --fix",
    "prettier": "prettier --write ./src"
  },
  "dependencies": {
    "@certusone/wormhole-sdk": "^0.9.21",
    "@cosmjs/cosmwasm-stargate": "^0.27.1",
    "@cosmjs/proto-signing": "^0.27.1",
    "@cosmjs/stargate": "^0.27.1",
    "@wormhole-foundation/connect-sdk": "*",
    "bs58": "^4.0.1",
    "cosmjs-types": "^0.8.0"
  },
  "devDependencies": {
    "@types/bs58": "^4.0.1",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.7.0",
    "lint-staged": "^13.1.2",
    "prettier": "2.8.4",
    "typescript": "^4.4.2"
  }
}
