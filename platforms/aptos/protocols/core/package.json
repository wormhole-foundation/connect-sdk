{
  "name": "@wormhole-foundation/connect-sdk-aptos-core",
  "version": "0.4.4-beta.4",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wormhole-foundation/connect-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/wormhole-foundation/connect-sdk/issues"
  },
  "homepage": "https://github.com/wormhole-foundation/connect-sdk#readme",
  "directories": {
    "test": "__tests__"
  },
  "license": "Apache-2.0",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "author": "",
  "description": "SDK for Solana, used in conjunction with @wormhole-foundation/connect-sdk",
  "files": [
    "dist"
  ],
  "keywords": [
    "wormhole",
    "sdk",
    "typescript",
    "connect",
    "aptos"
  ],
  "engines": {
    "node": ">=16"
  },
  "sideEffects": false,
  "scripts": {
    "build:cjs": "tsc -p ./tsconfig.cjs.json",
    "build:esm": "tsc -p ./tsconfig.esm.json",
    "build": "npm run build:cjs && npm run build:esm",
    "rebuild": "npm run clean && npm run build:cjs && npm run build:esm",
    "clean": "rm -rf ./dist && rm -f ./*.tsbuildinfo",
    "lint": "npm run prettier && eslint --fix",
    "prettier": "prettier --write ./src"
  },
  "devDependencies": {
    "nock": "^13.3.3"
  },
  "dependencies": {
    "@wormhole-foundation/connect-sdk": "0.4.4-beta.4",
    "@wormhole-foundation/connect-sdk-aptos": "0.4.4-beta.4"
  }
}