{
  "name": "@wormhole-foundation/sdk-cosmwasm-ibc",
  "version": "0.5.0-alpha.7",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wormhole-foundation/connect-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/wormhole-foundation/connect-sdk/issues"
  },
  "homepage": "https://github.com/wormhole-foundation/connect-sdk#readme",
  "directories": {
    "test": "tests"
  },
  "license": "Apache-2.0",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "description": "SDK for EVM chains, used in conjunction with @wormhole-foundation/sdk",
  "files": [
    "dist/esm",
    "dist/cjs"
  ],
  "keywords": [
    "wormhole",
    "sdk",
    "typescript",
    "connect",
    "ethereum",
    "moonbeam",
    "celo",
    "polygon",
    "fantom",
    "avalanche",
    "bsc",
    "evm"
  ],
  "engines": {
    "node": ">=16"
  },
  "sideEffects": true,
  "scripts": {
    "recover": "tsx ./scripts/recoverChannels.ts",
    "build:cjs": "tsc -p ./tsconfig.cjs.json  && echo '{\"type\":\"commonjs\"}' > dist/cjs/package.json",
    "build": "npm run build:esm && npm run build:cjs",
    "rebuild": "npm run clean && npm run build",
    "clean": "rm -rf ./dist && rm -rf ./.turbo",
    "lint": "npm run prettier && eslint --fix ./src --ext .ts",
    "prettier": "prettier --write ./src",
    "build:esm": "tsc -p ./tsconfig.esm.json"
  },
  "dependencies": {
    "@cosmjs/cosmwasm-stargate": "^0.32.0",
    "@cosmjs/stargate": "^0.32.0",
    "cosmjs-types": "^0.9.0",
    "@wormhole-foundation/sdk-connect": "0.5.0-alpha.7",
    "@wormhole-foundation/sdk-cosmwasm": "0.5.0-alpha.7",
    "@wormhole-foundation/sdk-cosmwasm-core": "0.5.0-alpha.7"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "types": "./dist/esm/index.d.ts",
      "require": "./dist/cjs/index.js"
    }
  }
}